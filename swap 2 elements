
public class Swap2ArrayEle {

	public static void main(String[] args) {
		int x[]= {11,21,3,4};

		for(int i=0;i<x.length-1;i++)
		{
			for(int j=0;j<x.length-i-1;j++)
			if(x[j]>x[j+1])
			{
			int temp=x[j];
			x[j]=x[j+1];
			x[j+1]=temp;
			}
			
			
		}

		
		for(int a:x)
			System.out.println(a);
	}

}
